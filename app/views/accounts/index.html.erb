<%= link_to "Sign Out", destroy_user_session_path(current_user), method: :delete %>
<%= link_to "Create New Account", new_account_path %>

<% unless @accounts.empty? %>
  <% @accounts.each do |account| %>
    <%= link_to account.name, account_path(account) %>
    <%= account.balance %>
  <% end %>
<% end %>
