<%= @account.name %>
<% unless @account.envelopes.empty? %>
  <% @account.envelopes.each do |envelope| %>
    <%= link_to envelope.name, account_envelope_path(@account, envelope) %>
    <%= link_to envelope %>
  <% end %>
<% end %>

<%= link_to 'Create New Envelope', new_account_envelope_path(@account) %>
